// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../db/database.db"
}

model User {
  id       String @id
  email    String @unique
  name     String
  password String
  photo    String
}

model Account {
  id          String @id
  userId      String
  name        String
  description String

  @@unique([name, userId])
}
